<% if (typeof errors !='undefined' ) { %>
    <% for (error of errors) { %>
        <div class="alert alert-danger col-4 offset-4" role="alert">
            <%= error.msg %>
        </div>
        <% } %>
            <% } %>

                <% if(success_msg !='' ) { %>
                    <div class="alert alert-success col-4 offset-4" role="alert">
                        <%= success_msg %>
                    </div>
                    <% } %>

                        <% if(error_msg !='' ) { %>
                            <div class="alert alert-danger col-4 offset-4" role="alert">
                                <%= error_msg %>
                            </div>
                            <% } %>